%%%%%%%%%%%%%%%%
% Aerodynamics %
%%%%%%%%%%%%%%%%

function [Fz_downforce] = Aerodynamics (air_t, atm_p, downforce_coef, drag_coef, area)
%--------------------------------------------------------------------------
%INPUTS
    air_d = 1.2255 * (15+273) / (air_temp + 273) * atm_p / 1000;
    speed = 130 * 0.27; %
%--------------------------------------------------------------------------
%CALCULATIONS
    
%--------------------------------------------------------------------------
%OUTPUTS
    Fz_downforce = 0.5 * air_d * area * downforce_coef * speed^2
end
%--------------------------------------------------------------------------
